{
  "parser": "babel-eslint",
  "extends": [
      "eslint:recommended", 
      "plugin:react/recommended" 
  ],    
  "env": {
      "es6": true,
      "node": true, 
      "browser": true
  },
  "plugins": ["react"], 
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true 
      }
  },
  "rules": {
    "react/prop-types": 0
  }
}